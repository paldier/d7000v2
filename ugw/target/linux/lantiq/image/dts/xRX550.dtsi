	ssx6@12000000 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "lantiq,ssx6", "simple-bus";
		reg = <0x12000000 0x2000000>;
		ranges = <0x0 0x12000000 0x02000000>;

		gcr@300000 {
			compatible = "lantiq,gcr-grx500";
			reg = <0x300000 0x8000>;
		};

		cpc@310000 {
			compatible = "lantiq,cpc-grx500";
			reg = <0x310000 0x6000>;
		};
	
		gic: gic@320000 {
			#interrupt-cells = <1>;
			interrupt-controller;
			compatible = "lantiq,gic-grx500";
			reg = <0x320000 0x20000>;
		};
		
		gcrcus@3f0000 {
			compatible = "lantiq,gcrcus-grx500";
			reg = <0x3f0000 0x200>;
		};

		watchdog@0,0 {
			#interrupt-cells = <1>;
			compatible = "lantiq,grx500wdt";
			reg = <0 0>;
			interrupt-parent = <&gic>;
			interrupts = <264>;
			timeout-sec = <5>;
		};
	};

	ssx7@a0000000 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "lantiq,ssx7", "simple-bus";
		reg = <0xa0000000 0x20000000>;
		ranges = <0x0 0xa0000000 0x20000000>;

		toe: toe@2000000 {
			#address-cells = <1>;
			#size-cells = <1>;
			compatible = "lantiq,toe-xrx500";
			reg = <0x2000000 0x800000>;
			interrupt-parent = <&gic>;
			interrupts = <79 80 81 82 83 190 191 192 193 194 195 196 197 198 199>;
		};

		mcpy: mcpy@2800000 {
			#address-cells = <1>;
			#size-cells = <1>;
			compatible = "lantiq,mcpy-xrx500";
			reg = <0x2800000 0x800000>;
			interrupt-parent = <&gic>;
			interrupts = <208 209 210 211 212 213 214 215 216>;
			lantiq,mcpy-yld = <200 201 202 203 204 205 206 207>;
			lantiq,mcpy-trnksz = <1>;
			lantiq,mcpy-irq-interval = <40>;
			lantiq,mcpy-prio = <1 0 1 0 1 0 1 0>; /* 1 is High prio */
			lantiq,mcpy-vpe  = <0 0 1 1 2 2 3 3>;
			lantiq,mcpy-ylden = <1 0 1 0 1 0 1 0>;
			lantiq,umt-period = <0x2000>;
		}; 
	};	
	
        dect {
                compatible = "lantiq,ltqdect";
                lantiq,dect-spi-int=<302>; /*271 GPIO base + GPIO 31 for cosic interrupt and driver Probe fn will look for string dect-spi-int */
                lantiq,dect-cs= <0>; /*DECT chip select port Number; This is used by SSC interface*/
                gpio-reset = <&gpio0 28 0>; /*COSIC Reset PIN connected to GPIO 28*/
                status = "okay";
        };

        dect-page {
                compatible = "lantiq,ltqdect-page";
                lantiq,pagepin= <&gpio0 29 0>; /*DECT Page Pin connected to GPIO 29*/
                lantiq,pagebit= <1>; /*DECT Page Bit: Bit info meant for DECT*/
                status = "okay";
        };

	ltq_swreset {
		compatible = "lantiq,ltq_swreset";
		swreset_pin = <&gpio0 0 0>;
		swreset_bit = <1>;
		status = "okay";
	};

	cpuclocks {
		compatible = "lantiq,scaling-frequencies";

		xrx350_cpuclocks: cpuclocks@0 {
			status = "disabled";
			lantiq,cpuclocks = <800000000 600000000 150000000 50000000>;
			lantiq,ddrclocks = <333000000 333000000 166000000 166000000>;
		}; 
	
		xrx500_cpuclocks: cpuclocks@1 {
			status = "disabled";
			lantiq,cpuclocks = <1200000000 800000000 150000000 50000000>;
			lantiq,ddrclocks = <400000000 333000000 166000000 166000000>;
		};
	}; 
