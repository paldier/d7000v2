	option enabled '1'
	option speed '1000'
	option duplex 'full'

config port 'eth0_3'
	option enabled '1'
	option speed '1000'
	option duplex 'full'

config port 'eth0_4'
	option enabled '1'
	option speed '100'
	option duplex 'full'
