From 5e85898a54de3300e436dbfdae34fce3e8686f8a Mon Sep 17 00:00:00 2001
From: "Brett T. Warden" <brett.t.warden@intel.com>
Date: Fri, 17 Apr 2015 17:26:13 -0700
Subject: [PATCH] lpc_ich: Add Cougar Mountain support

Add Cougar Mountain strings and PCI device ID
---
 drivers/mfd/lpc_ich.c |    6 ++++++
 1 file changed, 6 insertions(+)

diff --git a/drivers/mfd/lpc_ich.c b/drivers/mfd/lpc_ich.c
index b5bf020..37eb172 100644
--- a/drivers/mfd/lpc_ich.c
+++ b/drivers/mfd/lpc_ich.c
@@ -218,6 +218,7 @@ enum lpc_chipsets {
 	LPC_COLETO,	/* Coleto Creek */
 	LPC_WPT_LP,	/* Wildcat Point-LP */
 	LPC_CHERRYTRAIL,/* Cherry Trail SoC */
+	LPC_COUGARMOUNTAIN,/* Cougar Mountain SoC */
 };
 
 static struct lpc_ich_info lpc_chipset_info[] = {
@@ -522,6 +523,10 @@ static struct lpc_ich_info lpc_chipset_info[] = {
 		.name = "Cherry Trail SoC",
 		.iTCO_version = 3,
 	},
+	[LPC_COUGARMOUNTAIN] = {
+		.name = "Cougar Mountain SoC",
+		.iTCO_version = 3,
+	},
 };
 
 /*
@@ -749,6 +754,7 @@ static const struct pci_device_id lpc_ich_ids[] = {
 	{ PCI_VDEVICE(INTEL, 0x9cc7), LPC_WPT_LP},
 	{ PCI_VDEVICE(INTEL, 0x9cc9), LPC_WPT_LP},
 	{ PCI_VDEVICE(INTEL, 0x229c), LPC_CHERRYTRAIL},
+	{ PCI_VDEVICE(INTEL, 0x2b9c), LPC_COUGARMOUNTAIN},
 	{ 0, },			/* End of list */
 };
 MODULE_DEVICE_TABLE(pci, lpc_ich_ids);
-- 
