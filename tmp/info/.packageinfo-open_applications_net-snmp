	menu "overwrite package version"
		depends on PACKAGE_net-snmp
	config net-snmp_USE_CUSTOM_VERSION
		depends on PACKAGE_net-snmp
		bool "Use custom package version"
		default n
	config net-snmp_CUSTOM_VERSION
		depends on net-snmp_USE_CUSTOM_VERSION
