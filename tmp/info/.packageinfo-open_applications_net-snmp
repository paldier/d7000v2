		default "5.1"
	endmenu
	menu "custom source directory"
		depends on PACKAGE_net-snmp
	config net-snmp_USE_CUSTOM_SOURCE_DIR
		depends on PACKAGE_net-snmp
		bool "Use custom source directory"
		default n
