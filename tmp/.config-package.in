menu "Web"
	menuconfig PACKAGE_mini-httpd
		tristate "mini-httpd............................................ A small web server"
		default y if DEFAULT_mini-httpd
		default m if ALL
		select PACKAGE_libpthread if USE_GLIBC
		select PACKAGE_librt if USE_GLIBC
		select PACKAGE_libssp if SSP_SUPPORT
		select PACKAGE_libc if !PACKAGE_ecos
		help
		 mini_httpd is a small HTTP server. Its performance is not great, but for 
		  low or medium traffic sites it's quite adequate. It implements all the 
		  basic features of an HTTP server, including: 
		  - GET, HEAD, and POST methods
		  - CGI
		  - Basic authentication
		  - Security against ".." filename snooping
		  - The common MIME types
		  - Trailing-slash redirection
		  - index.html, index.htm, index.cgi
		  - Directory listings
		  - Multihoming / virtual hosting
		  - Standard logging
		  - Custom error pages
		  It can also be configured to do IPv6.
		 http://www.acme.com/software/mini_httpd/
		 


	   	menu "overwrite package version"
			depends on PACKAGE_mini-httpd
		config mini-httpd_USE_CUSTOM_VERSION
			depends on PACKAGE_mini-httpd
			bool "Use custom package version"
			default n
		config mini-httpd_CUSTOM_VERSION
			depends on mini-httpd_USE_CUSTOM_VERSION
			string "mini-httpd version as string (default version: 1.19)"
			default "1.19"
		endmenu
	   	menu "custom source directory"
			depends on PACKAGE_mini-httpd
		config mini-httpd_USE_CUSTOM_SOURCE_DIR
			depends on PACKAGE_mini-httpd
			bool "Use custom source directory"
			default n
		config mini-httpd_CUSTOM_SOURCE_DIR
			depends on mini-httpd_USE_CUSTOM_SOURCE_DIR
			string "Custom source directory"
			default ""
		endmenu

