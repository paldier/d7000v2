install: all
	rm -r -f $(TARGETDIR)/$(WWWROOT_PATH)
	install -d $(TARGETDIR)/$(WWWROOT_PATH)
	mkdir -p $(TARGETDIR)/$(WWWROOT_PATH)
	mkdir -p $(TARGETDIR)/$(WWWROOT_PATH)/css
	mkdir -p $(TARGETDIR)/$(WWWROOT_PATH)/img
	mkdir -p $(TARGETDIR)/$(WWWROOT_PATH)/script
	mkdir -p $(TARGETDIR)/$(WWWROOT_PATH)/help
	mkdir -p $(TARGETDIR)/$(WWWROOT_PATH)/cgi-bin
	install -m 644 $(addprefix ./html/, $(PAGES)) $(TARGETDIR)/$(WWWROOT_PATH)
	install -m 644 $(addprefix ./html/, $(CSS_PAGES)) $(TARGETDIR)/$(WWWROOT_PATH)/css
	install -m 644 $(addprefix ./html/, $(IMG_PAGES)) $(TARGETDIR)/$(WWWROOT_PATH)/img
	install -m 644 $(addprefix ./html/, $(HELP_PAGES)) $(TARGETDIR)/$(WWWROOT_PATH)/help
	install -m 644 $(addprefix ./html/, $(SCRIPT_PAGES)) $(TARGETDIR)/$(WWWROOT_PATH)/script
	install -m 644 $(addprefix ./html/, $(SCRIPT_PAGES)) $(TARGETDIR)/$(WWWROOT_PATH)/cgi-bin
	install -m 755 $(addprefix ./html/, $(CGI_BIN)) $(TARGETDIR)/$(WWWROOT_PATH)/cgi-bin
	$(STRIP) $(TARGETDIR)/www/cgi-bin/readycloud_control.cgi
	install -m 644 $(STR_TBL_FILE) $(TARGETDIR)/$(WWWROOT_PATH)

clean:


